# vim: ts=4:sw=4
Host 10.*
    CheckHostIP no
    ForwardAgent yes
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host workhost
    ForwardAgent yes
    Hostname workhost.b1-managed-service-ccee.c.eu-nl-1.cloud.sap

Host marmelade.local marmelade
    User knizek
    ForwardAgent no
    ControlMaster auto
    ControlPath ~/.ssh/%C.sock
    ControlPersist yes

Host toast.local toast
    User silvio
    ForwardAgent no
    ControlMaster auto
    ControlPath ~/.ssh/%C.sock
    ControlPersist yes
